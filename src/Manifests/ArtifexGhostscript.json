{
	"Name": "Artifex Ghostscript",
	"Source": "https://www.ghostscript.com/",
        
	"Get": {
		"Uri": "https://api.github.com/repos/ArtifexSoftware/ghostpdl-downloads/releases/latest",
		"MatchVersion": "(\\d+(\\.\\d+){1,4}).*",
		"MatchFileTypes": "\\.exe$",
                "VersionTag": "name"
	},
	"Install": {
		"Setup": "gs*.exe",
		"Physical": {
			"Arguments": "/S",
			"PostInstall": []
		},
		"Virtual": {
			"Arguments": "/S",
			"PostInstall": []
		}
	}
}
